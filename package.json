{
  "name": "react-ts-next-starter",
  "version": "1.0.0",
  "description": "Starter project with React Typescript and NextJs",
  "repository": "https://github.com/JFeremy/React-Ts-Next-Starter",
  "author": "Jérémy <fumeron.jeremy@live.fr>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "dev": "next dev",
    "build": "next build && next export",
    "postbuild": "npm run 404",
    "start": "next start",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "yarn lint --fix",
    "format": "prettier-standard --format",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "synchro": "git checkout main && git pull"
  },
  "standard": {
    "parser": "@babel/eslint-parser"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "i18next": "^23.4.1",
    "next": "^13.x",
    "next-i18next": "^14.0.0",
    "next-language-detector": "^1.0.2",
    "react": "^18.x",
    "react-dom": "^18.x",
    "react-i18next": "^13.0.2",
    "react-swipeable": "^7.x"
  },
  "devDependencies": {
    "@babel/core": "^7.x",
    "@babel/eslint-parser": "^7.x",
    "@babel/preset-typescript": "^7.x",
    "@testing-library/jest-dom": "^5.x",
    "@testing-library/react": "^14.x",
    "@types/jest": "^29.x",
    "@types/node": "^20.x",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "autoprefixer": "^10.x",
    "babel-loader": "^9.x",
    "flowbite": "^1.x",
    "flowbite-react": "^0.x",
    "husky": "^8.x",
    "jest": "^29.x",
    "lint-staged": "^13.x",
    "plop": "^3.x",
    "postcss": "^8.x",
    "prettier": "^3.x",
    "prettier-standard": "^16.x",
    "snazzy": "^9.x",
    "standard": "^17.x",
    "tailwindcss": "^3.x",
    "ts-standard": "^12.x",
    "typescript": "^5.x"
  }
}
